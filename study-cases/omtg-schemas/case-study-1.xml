<?xml version="1.0" encoding="UTF-8"?>
<omtg-conceptual-schema xsi:noNamespaceSchemaLocation="omtg-schema-template.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<classes>
		<class>
			<name>Region</name>
			<type>conventional</type>
			<attributes>
				<attribute>
					<name>code</name>
					<type>number</type>
					<key>true</key>
					<length>5</length>
					<scale>0</scale>
				</attribute>
				<attribute>
					<name>name</name>
					<type>varchar2</type>
					<length>100</length>
					<not-null>true</not-null>
				</attribute>
			</attributes>
		</class>
		<class>
			<name>RegionPoint</name>
			<type>point</type>
			<attributes>
			</attributes>
		</class>
		<class>
			<name>RegionBoundary</name>
			<type>planar-subdivision</type>
			<attributes>
			</attributes>
		</class>
		<class>
			<name>Neighborhood</name>
			<type>polygon</type>
			<attributes>
				<attribute>
					<name>code</name>
					<type>number</type>
					<key>true</key>
					<length>5</length>
					<scale>0</scale>
				</attribute>
				<attribute>
					<name>name</name>
					<type>varchar2</type>
					<length>100</length>
					<not-null>true</not-null>
				</attribute>
			</attributes>
		</class>
		<class>
			<name>Block</name>
			<type>polygon</type>
			<attributes>
				<attribute>
					<name>code</name>
					<type>number</type>
					<key>true</key>
					<length>5</length>
					<scale>0</scale>
				</attribute>
			</attributes>
		</class>
		<class>
			<name>StreetCrossing</name>
			<type>node</type>
			<attributes>
				<attribute>
					<name>id</name>
					<type>number</type>
					<key>true</key>
					<length>5</length>
					<scale>0</scale>
				</attribute>
			</attributes>
		</class>
		<class>
			<name>StreetSegment</name>
			<type>un-line</type>
			<attributes>
				<attribute>
					<name>id</name>
					<type>number</type>
					<key>true</key>
					<length>5</length>
					<scale>0</scale>
				</attribute>
				<attribute>
					<name>type</name>
					<type>varchar2</type>
					<length>100</length>
				</attribute>
			</attributes>
		</class>
		<class>
			<name>Thoroughfare</name>
			<type>conventional</type>
			<attributes>
				<attribute>
					<name>id</name>
					<type>number</type>
					<key>true</key>
					<length>5</length>
					<scale>0</scale>
				</attribute>
				<attribute>
					<name>name</name>
					<type>varchar2</type>
					<length>100</length>
					<not-null>true</not-null>
				</attribute>
			</attributes>
		</class>
		<class>
			<name>Building</name>
			<type>point</type>
			<attributes>
				<attribute>
					<name>id</name>
					<type>number</type>
					<key>true</key>
					<length>5</length>
					<scale>0</scale>
				</attribute>
				<attribute>
					<name>number</name>
					<type>number</type>
					<length>5</length>
					<scale>0</scale>
					<not-null>true</not-null>
				</attribute>
			</attributes>
		</class>
		<class>
			<name>Residence</name>
			<type>point</type>
			<attributes>
			</attributes>
		</class>
		<class>
			<name>Commerce</name>
			<type>point</type>
			<attributes>
				<attribute>
					<name>commerceType</name>
					<type>varchar2</type>
					<length>100</length>
				</attribute>
			</attributes>
		</class>
		<class>
			<name>Industry</name>
			<type>point</type>
			<attributes>
				<attribute>
					<name>industryType</name>
					<type>varchar2</type>
					<length>100</length>
				</attribute>
				<attribute>
					<name>industrySize</name>
					<type>varchar2</type>
					<length>100</length>
					<domain>
						<value>small</value>
						<value>medium</value>
						<value>large</value>
					</domain>
				</attribute>
			</attributes>
		</class>
	</classes>
	<relationships>
		<conceptual-generalization>
			<superclass>Region</superclass>
			<scale-shape>shape</scale-shape>
			<disjointness>overlapping</disjointness>
			<subclasses>
				<subclass>RegionPoint</subclass>
				<subclass>RegionBoundary</subclass>
			</subclasses>
		</conceptual-generalization>
		<spatial-aggregation>
			<class1>RegionBoundary</class1>
			<class2>Neighborhood</class2>
		</spatial-aggregation>
		<topological>
			<spatial-relations>
				<spatial-relation>contains</spatial-relation>
				<spatial-relation>touch</spatial-relation>
			</spatial-relations>
			<class1>RegionBoundary</class1>
			<cardinality1>
				<min>1</min>
				<max>1</max>
			</cardinality1>
			<class2>StreetCrossing</class2>
			<cardinality2>
				<min>1</min>
				<max>n</max>
			</cardinality2>
		</topological>
		<network>
			<name>StreetNetwork</name>
			<class1>StreetCrossing</class1>
			<class2>StreetSegment</class2>
		</network>
		<topological>
			<spatial-relations>
				<spatial-relation>overlapbdyintersect</spatial-relation>
				<spatial-relation>overlapbdydisjoint</spatial-relation>
				<spatial-relation>inside</spatial-relation>
				<spatial-relation>coveredby</spatial-relation>
			</spatial-relations>
			<class1>StreetSegment</class1>
			<cardinality1>
				<min>1</min>
				<max>n</max>
			</cardinality1>
			<class2>Neighborhood</class2>
			<cardinality2>
				<min>1</min>
				<max>n</max>
			</cardinality2>
		</topological>
		<topological>
			<spatial-relations>
				<spatial-relation>contains</spatial-relation>
				<spatial-relation>covers</spatial-relation>
			</spatial-relations>
			<class1>Neighborhood</class1>
			<cardinality1>
				<min>1</min>
				<max>1</max>
			</cardinality1>
			<class2>Block</class2>
			<cardinality2>
				<min>0</min>
				<max>n</max>
			</cardinality2>
		</topological>
		<topological>
			<spatial-relations>
				<spatial-relation>contains</spatial-relation>
			</spatial-relations>
			<class1>Block</class1>
			<cardinality1>
				<min>1</min>
				<max>1</max>
			</cardinality1>
			<class2>Building</class2>
			<cardinality2>
				<min>1</min>
				<max>n</max>
			</cardinality2>
		</topological>
		<topological>
			<spatial-relations>
				<spatial-relation>near</spatial-relation>
				<distance>50</distance>
				<unit>m</unit>
			</spatial-relations>
			<class1>Building</class1>
			<cardinality1>
				<min>1</min>
				<max>n</max>
			</cardinality1>
			<class2>StreetSegment</class2>
			<cardinality2>
				<min>1</min>
				<max>n</max>
			</cardinality2>
		</topological>
		<conventional>
			<name>belongs</name>
			<class1>StreetSegment</class1>
			<cardinality1>
				<min>1</min>
				<max>n</max>
			</cardinality1>
			<class2>Thoroughfare</class2>
			<cardinality2>
				<min>1</min>
				<max>1</max>
			</cardinality2>
		</conventional>
		<generalization>
			<superclass>Building</superclass>
			<participation>total</participation>
			<disjointness>disjoint</disjointness>
			<subclasses>
				<subclass>Residence</subclass>
				<subclass>Commerce</subclass>
				<subclass>Industry</subclass>
			</subclasses>
		</generalization>
		<user-restriction>
			<class1>StreetSegment</class1>
			<spatial-relations>
				<spatial-relation>overlapbdyintersect</spatial-relation>
				<spatial-relation>overlapbdydisjoint</spatial-relation>
				<spatial-relation>inside</spatial-relation>
				<spatial-relation>coveredby</spatial-relation>
			</spatial-relations>
			<can-occur>false</can-occur>
			<class2>Block</class2>
		</user-restriction>
	</relationships>
</omtg-conceptual-schema>