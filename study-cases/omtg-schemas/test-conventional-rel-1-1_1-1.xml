<?xml version="1.0" encoding="UTF-8"?>
<omtg-conceptual-schema xsi:noNamespaceSchemaLocation="omtg-schema-template.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<classes>
		<class>
			<name>cidade</name>
			<type>polygon</type>
			<attributes>
				<attribute>
					<name>idCidade</name>
					<type>number</type>
					<key>true</key>
					<length>5</length>
					<scale>0</scale>
				</attribute>
				<attribute>
					<name>nomeCidade</name>
					<type>varchar2</type>
					<key>true</key>
					<length>100</length>
				</attribute>
				<attribute>
					<name>codEstado</name>
					<type>varchar2</type>
					<length>100</length>
					<not-null>true</not-null>
					<domain>
						<value>AL</value>
						<value>MG</value>
						<value>PB</value>
					</domain>
				</attribute>
				<attribute>
					<name>telefone</name>
					<type>varchar2</type>
					<length>10</length>
					<size>10</size>
				</attribute>
			</attributes>
		</class>
		<class>
			<name>bairro</name>
			<type>point</type>
			<attributes>
				<attribute>
					<name>idBairro</name>
					<type>number</type>
					<key>true</key>
					<length>5</length>
					<scale>0</scale>
				</attribute>
			</attributes>
		</class>
	</classes>
	<relationships>
		<conventional>
			<name>contem</name>
			<class1>cidade</class1>
			<cardinality1>
				<min>1</min>
				<max>1</max>
			</cardinality1>
			<class2>bairro</class2>
			<cardinality2>
				<min>1</min>
				<max>1</max>
			</cardinality2>
		</conventional>
	</relationships>
</omtg-conceptual-schema>