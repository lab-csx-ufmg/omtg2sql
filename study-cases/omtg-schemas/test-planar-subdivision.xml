<?xml version="1.0" encoding="UTF-8"?>
<omtg-conceptual-schema xsi:noNamespaceSchemaLocation="omtg-schema-template.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<classes>
		<class>
			<name>cidade</name>
			<type>planar-subdivision</type>
			<attributes>
				<attribute>
					<name>idCidade</name>
					<type>number</type>
					<key>true</key>
					<length>5</length>
					<scale>0</scale>
				</attribute>
				<attribute>
					<name>nomeCidade</name>
					<type>varchar2</type>
					<key>true</key>
					<length>50</length>
					<not-null>true</not-null>
				</attribute>
				<attribute>
					<name>codEstado</name>
					<type>varchar2</type>
					<length>50</length>
					<default>AL</default>
					<domain>
						<value>AL</value>
						<value>MG</value>
						<value>PB</value>
					</domain>
				</attribute>
				<attribute>
					<name>telefone</name>
					<type>varchar2</type>
					<length>10</length>
					<size>10</size>
				</attribute>
			</attributes>
		</class>
		<class>
			<name>bairro</name>
			<type>point</type>
			<attributes>
				<attribute>
					<name>idBairro</name>
					<type>number</type>
					<key>true</key>
					<length>5</length>
					<scale>0</scale>
				</attribute>
			</attributes>
		</class>
	</classes>
	<relationships>
		<spatial-aggregation>
			<class1>cidade</class1>
			<class2>bairro</class2>
		</spatial-aggregation>
	</relationships>
</omtg-conceptual-schema>